{
	"name": "slidev-theme-prussianblue",
	"version": "0.0.8",
	"keywords": [
		"slidev-theme",
		"slidev"
	],
	"scripts": {
		"build": "slidev build example.md",
		"dev": "slidev example.md --open",
		"export": "slidev export example.md",
		"screenshot": "slidev export example.md --format png"
	},
	"dependencies": {
		"@slidev/types": "^0.44.0",
		"codemirror-theme-vars": "^0.1.2",
		"prism-theme-vars": "^0.2.4",
		"theme-vitesse": "^0.7.3"
	},
	"devDependencies": {
		"@slidev/cli": "^0.44.0",
		"playwright-chromium": "^1.39.0"
	},
	"engines": {
		"node": ">=18.0.0",
		"slidev": ">=0.19.3"
	},
	"//": "Learn more: https://sli.dev/themes/write-a-theme.html",
	"slidev": {
		"colorSchema": "light",
		"highlighter": "all",
		"defaults": {
			"fonts": {
				"sans": "Nunito Sans",
				"mono": "Fira Code"
			},
			"hightlighter": "prism"
		}
	}
}
